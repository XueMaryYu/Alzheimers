# Bowtie2
manual:http://bowtie-bio.sourceforge.net/bowtie2/manual.shtml
~command line: mkdir human_genome_index
               bowtie2-build human_genome.fa human_genome_index

# tophat 
manual:https://ccb.jhu.edu/software/tophat/manual.shtml
~command line: tophat -p 20 bowtie_index reads1.fq,reads2.fq
               (tophat human_genome_index/human_genome_index reads.fasta)

# Samtools
manual:http://www.htslib.org/doc/samtools-1.2.html
~command line: samtools fasta file.bam
               samtools fastq file.bam
